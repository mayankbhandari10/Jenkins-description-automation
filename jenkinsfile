pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'make' // Example build step
            }
        }
        stage('Post-build Actions') {
            steps {
                // Use the Log Parser Plugin to parse the build log
                logParser(parserName: 'MyLogParser', failBuildOnError: true)
            }
        }
    }
}

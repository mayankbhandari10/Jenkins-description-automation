pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                script {
                    // Run your build steps here

                    // Capture the console output
                    def consoleLog = currentBuild.rawBuild.getLog(1000)

                    // Convert the console output (ArrayList) to a string
                    def consoleLogString = consoleLog.join('\n')

                    // Print the console output
                    echo "Console Log:"
                    echo consoleLogString
                }
            }
        }
    }
}
